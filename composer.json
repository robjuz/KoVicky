{
    "name": "robjuz/cakephp-kovicky",
    "description": "wiki plugin for CakePHP",
    "type": "cakephp-plugin",
    "version" : 1.0.0,
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
    	"cakephp/migrations": "@stable",
        "Holt59/cakephp3-bootstrap-helpers": "dev-master",
        "imagine/imagine": "~0.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "KoVicky\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KoVicky\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
